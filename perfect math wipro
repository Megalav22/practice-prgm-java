
#include <stdio.h>

int main()
{
    int n, modvalue, arrlist[100],rem,sum=0;
    printf("Enter the size of the  array");
    scanf("%d",&n);
    printf("enter the value to divide the list");
    scanf("%d",&modvalue);
    printf("enter the elements to an array\n");
    for(int i=0;i<n;i++)
    {
        //printf("enter the elements to an array");
        scanf("%d",&arrlist[i]);
    }
    for(int i=0;i<n;i++)
    {
      rem=arrlist[i]%4;
      sum+=rem;
    }
    printf("the sum is %d",sum);
    return 0;
    
    
}

